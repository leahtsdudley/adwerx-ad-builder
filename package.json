{
  "name": "adwerx-ad-builder",
  "description": "Generate customized ad assets.",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "adwerx-ad-builder",
    "manifest": "src/manifest.json",
    "main": "adwerx-ad-builder.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Leah Dudley <leahtsdudley@gmail.com>"
}
